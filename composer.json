{
  "name": "soluble/wallit",
  "description": "Middleware for JWT auth",
  "license": "MIT",
  "keywords": [
    "auth",
    "jwt",
    "psr-7",
    "psr-15",
    "expressive",
    "middleware"
  ],
  "homepage": "http://soluble.io",
  "type": "library",
  "authors": [
    {
      "name": "SÃ©bastien Vanvelthem",
      "homepage": "https://github.com/belgattitude"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.1",
    "fig/http-message-util": "^1.1.2",
    "lcobucci/jwt": "^3.2.2",
    "psr/container": "^1.0",
    "psr/http-message": "^1.0.1",
    "psr/http-server-middleware": "^1.0"
  },
  "require-dev" : {
    "dflydev/fig-cookies": "^1.0",
    "filp/whoops": "^2.1",
    "friendsofphp/php-cs-fixer": "^2.8.1",
    "guzzlehttp/guzzle": "^6.2.3",
    "infection/infection": "^0.7.0",
    "jangregor/phpstan-prophecy": "^0.1.0",
    "mockery/mockery": "^1.0.0",
    "phpstan/phpstan": "^0.9.2",
    "phpstan/phpstan-phpunit": "^0.9.4",
    "phpstan/phpstan-strict-rules": "^0.9.0",
    "phpunit/phpunit": "^6.5.5",
    "ramsey/uuid": "^3.7",
    "roave/security-advisories": "dev-master",
    "zendframework/zend-config-aggregator": "^1.0",
    "zendframework/zend-expressive": "^3.0.0alpha5 || ^3.0",
    "zendframework/zend-expressive-fastroute": "^3.0.0alpha1 || ^3.0",
    "zendframework/zend-expressive-helpers": "^5.0.0alpha3 || ^5.0",
    "zendframework/zend-expressive-twigrenderer": "^2.0.0alpha1 || ^2.0",
    "zendframework/zend-servicemanager": "^3.3",
    "zendframework/zend-stdlib": "^3.1",
    "zendframework/zend-stratigility": "3.0.0alpha3"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "conflict": {
    "container-interop/container-interop": "<1.2.0"
  },
  "autoload": {
    "psr-4": {
      "Soluble\\Wallit\\": "src/Soluble/Wallit"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SolubleTest\\Wallit\\": "tests/SolubleTest/Wallit",
      "ExpressiveWallitApp\\": "tests/server/expressive/src/ExpressiveWallitApp",
      "ExpressiveWallitSmokeTest\\": "tests/ExpressiveWallitSmokeTest"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "zf": {
      "component": "Soluble\\Wallit",
      "config-provider": "Soluble\\Wallit\\Config\\ConfigProvider"
    }
  },
  "scripts": {
    "check": [
      "@phpstan",
      "@phpds",
      "@cs-check",
      "@test"
    ],
    "fix": [
      "@cs-fix"
    ],
    "test": "vendor/bin/phpunit --colors=always",
    "test:mutation": "vendor/bin/infection --test-framework=phpunit --verbose --min-msi=85 --min-covered-msi=85 --threads=4",
    "test:serve": "php -S localhost:7999 -t tests/server/expressive/public",
    "cs-check": "vendor/bin/php-cs-fixer --diff --dry-run -v fix",
    "cs-fix": "vendor/bin/php-cs-fixer -v fix",
    "phpstan": "vendor/bin/phpstan analyse -l max -c phpstan.neon src tests",
    "build-doc": "mkdocs build && apigen.phar generate --config=./.apigen.yml"
  },
  "suggest": {
    "zendframework/zend-diactoros": "PSR HTTP Message implementations",
    "zendframework/zend-expressive": "PSR-7 Middleware Microframework based on Stratigility"
  },
  "archive": {
    "exclude": ["infection.json.dist", "tests", "docs", ".travis", ".travis.yml", ".codeclimate.yml", ".coveralls.yml", ".scrutinizer.yml", ".php_cs", ".gitignore", "phpcs.xml", "infection.json.dist", "phpunit.xml.dist"]
  }
}
