{
  "name": "soluble/wallit",
  "description": "Middleware for JWT auth",
  "license": "MIT",
  "keywords": [
    "auth",
    "jwt",
    "psr-7",
    "psr-15",
    "expressive",
    "middleware"
  ],
  "homepage": "http://soluble.io",
  "type": "library",
  "authors": [
    {
      "name": "SÃ©bastien Vanvelthem",
      "homepage": "https://github.com/belgattitude"
    }
  ],

  "require": {
    "php": "^7.1",
    "fig/http-message-util": "^1.1.2",
    "http-interop/http-middleware": null,
    "lcobucci/jwt": "^3.2.2",
    "psr/container": "^1.0",
    "psr/http-message": "^1.0.1",
    "webimpress/http-middleware-compatibility": "^0.1 || ^1.0"
  },

  "require-dev" : {
    "dflydev/fig-cookies": "^1.0",
    "filp/whoops": "^2.1.12",
    "friendsofphp/php-cs-fixer": "^2.7.1",
    "guzzlehttp/guzzle": "^6.2.3",
    "mockery/mockery": "^1.0.0",
    "pds/skeleton": "@dev",
    "phpstan/phpstan": "^0.8.5",
    "phpunit/phpunit": "^6.4.3",
    "ramsey/uuid": "^3.7",
    "thecodingmachine/phpstan-strict-rules": "^0.8.3",
    "http-interop/http-middleware": "^0.4.1",
    "zendframework/zend-config-aggregator": "^1.0.0",
    "zendframework/zend-db": "^2.8",
    "zendframework/zend-expressive": "^2.0.5",
    "zendframework/zend-expressive-fastroute": "^2.0",
    "zendframework/zend-expressive-helpers": "^4.2.0",
    "zendframework/zend-expressive-twigrenderer": "^1.4.0",
    "zendframework/zend-servicemanager": "^3.3",
    "zendframework/zend-session": "^2.7.3"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "conflict": {
    "container-interop/container-interop": "<1.2.0"
  },
  "autoload": {
    "psr-4": {
      "Soluble\\Wallit\\": "src/Soluble/Wallit"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SolubleTest\\Wallit\\": "tests/SolubleTest/Wallit",
      "ExpressiveWallitApp\\": "tests/expressive/src/ExpressiveWallitApp",
      "ExpressiveWallitSmokeTest\\": "tests/ExpressiveWallitSmokeTest"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "zf": {
      "component": "Soluble\\Wallit",
      "config-provider": "Soluble\\Wallit\\Config\\ConfigProvider"
    }
  },
  "scripts": {
    "check": [
      "@phpstan",
      "@phpds",
      "@cs-check",
      "@test"
    ],
    "fix": [
      "@cs-fix"
    ],
    "test": "vendor/bin/phpunit --colors=always",
    "test:serve": "php -S localhost:7999 -t tests/expressive/public",
    "cs-check": "vendor/bin/php-cs-fixer --diff --dry-run -v fix",
    "cs-fix": "vendor/bin/php-cs-fixer -v fix",
    "phpstan": "vendor/bin/phpstan analyse -l max -c phpstan.neon src tests",
    "phpds": "vendor/bin/pds-skeleton validate",
    "build-doc": "mkdocs build && apigen.phar generate --config=./.apigen.yml"
  },
  "suggest": {
    "monolog/monolog": "PSR-3 compatible logger"
  },
  "archive": {
    "exclude": ["tests", "docs", ".travis", ".travis.yml", ".codeclimate.yml", ".coveralls.yml", ".scrutinizer.yml", ".php_cs", ".gitignore", "phpcs.xml"]
  }
}
